    "custom_liquid_BnKDeC": {
      "type": "custom-liquid",
      "name": "t:sections.custom_liquid.presets.custom_liquid.name",
      "settings": {
        "color_scheme": "",
        "separate_section_with_border": true,
        "remove_vertical_spacing": false,
        "remove_horizontal_spacing": false,
        "liquid": "<!-- Programs media rail -->\n<section class=\"mv\" aria-label=\"Programs media rail\">\n  <div class=\"mv-head\"></div>\n\n  <div class=\"mv-viewport\">\n    <div class=\"mv-track\" id=\"mvTrack\">\n\n      <!-- CARD 1 -->\n      <figure class=\"mv-card\">\n        <video class=\"mv-video\" playsinline muted loop preload=\"metadata\" aria-label=\"Hip opener 8–10 min\">\n          <source src=\"https://cdn.prod.website-files.com/6630f2d54f73b2b2fc538ab6%2F6671ad575d3b7145555971f6_Routine%20Content%20Singles%20v1%2001-transcode.webm\" type=\"video/webm\">\n        </video>\n        <figcaption class=\"mv-caption\">\n          <span class=\"mv-badge\">Beginner</span>\n          <h3 class=\"mv-cap-title\">Hip opener</h3>\n          <p class=\"mv-cap-meta\">8–10 min · No gear</p>\n        </figcaption>\n      </figure>\n\n      <!-- CARD 2 -->\n      <figure class=\"mv-card\">\n        <video class=\"mv-video\" playsinline muted loop preload=\"metadata\" aria-label=\"Full-body reset 6 min\">\n          <source src=\"https://cdn.prod.website-files.com/6630f2d54f73b2b2fc538ab6%2F6671af5c89426ab26f0ffa8e_Routine%20Content%20Singles%20v1%2004-transcode.webm\" type=\"video/webm\">\n        </video>\n        <figcaption class=\"mv-caption\">\n          <span class=\"mv-badge\">Quick</span>\n          <h3 class=\"mv-cap-title\">Full-body reset</h3>\n          <p class=\"mv-cap-meta\">6 min · Daily</p>\n        </figcaption>\n      </figure>\n\n      <!-- CARD 3 -->\n      <figure class=\"mv-card\">\n        <video class=\"mv-video\" playsinline muted loop preload=\"metadata\" aria-label=\"Thoracic flow 10–12 min\">\n          <source src=\"https://cdn.prod.website-files.com/6630f2d54f73b2b2fc538ab6%2F6671afdcfb4dfce86af3db85_Routine%20Content%20Singles%20v1%2005-transcode.webm\" type=\"video/webm\">\n        </video>\n        <figcaption class=\"mv-caption\">\n          <span class=\"mv-badge\">Athletes</span>\n          <h3 class=\"mv-cap-title\">Thoracic flow</h3>\n          <p class=\"mv-cap-meta\">10–12 min · Spine</p>\n        </figcaption>\n      </figure>\n\n      <!-- CARD 4 -->\n      <figure class=\"mv-card\">\n        <video class=\"mv-video\" playsinline muted loop preload=\"metadata\" aria-label=\"Shoulder release 7 min\">\n          <source src=\"https://cdn.prod.website-files.com/6630f2d54f73b2b2fc538ab6%2F6671aede3b47c7ef2165c060_Routine%20Content%20Singles%20v1%2003-transcode.webm\" type=\"video/webm\">\n        </video>\n        <figcaption class=\"mv-caption\">\n          <span class=\"mv-badge\">Desk</span>\n          <h3 class=\"mv-cap-title\">Shoulder release</h3>\n          <p class=\"mv-cap-meta\">7 min · Neck/upper back</p>\n        </figcaption>\n      </figure>\n\n      <!-- CARD 5 -->\n      <figure class=\"mv-card\">\n        <video class=\"mv-video\" playsinline muted loop preload=\"metadata\" aria-label=\"Glute & ham reset 8 min\">\n          <source src=\"https://cdn.prod.website-files.com/6630f2d54f73b2b2fc538ab6%2F6671b07fc0b367eaf2f4227b_Routine%20Content%20Singles%20v1%2010-transcode.webm\" type=\"video/webm\">\n        </video>\n        <figcaption class=\"mv-caption\">\n          <span class=\"mv-badge\">Recovery</span>\n          <h3 class=\"mv-cap-title\">Glute & ham reset</h3>\n          <p class=\"mv-cap-meta\">8 min · Post-workout</p>\n        </figcaption>\n      </figure>\n    </div>\n\n    <!-- Mobile nav -->\n    <button class=\"mv-nav mv-prev\" aria-label=\"Previous\">‹</button>\n    <button class=\"mv-nav mv-next\" aria-label=\"Next\">›</button>\n  </div>\n</section>\n\n<style>\n  .mv{\n    --gap:12px; --radius:14px; --ink:#0f0f10; --line:rgba(0,0,0,.10);\n    --cap-bg: linear-gradient(180deg, rgba(0,0,0,0) 20%, rgba(0,0,0,.55) 78%);\n    padding:10px 0; color:var(--ink);\n  }\n  .mv-viewport{ position:relative; overflow:hidden; }\n\n  /* Rail (mobile: single row) */\n  .mv-track{\n    display:flex; flex-wrap:nowrap; gap:var(--gap);\n    overflow-x:auto; scroll-snap-type:x mandatory;\n    -webkit-overflow-scrolling:touch; padding:0 10px;\n    scrollbar-width:none;\n  }\n  .mv-track::-webkit-scrollbar{ display:none; }\n\n  /* Card = BOX on mobile (1:1), taller on desktop (4:5) */\n  .mv-card{\n    position:relative; flex:0 0 88%; min-width:260px; max-width:520px;\n    scroll-snap-align:center; border-radius:var(--radius); overflow:hidden;\n    border:1px solid var(--line); background:#000;\n    aspect-ratio:1/1; /* square on phones */\n    box-shadow:0 6px 18px rgba(0,0,0,.08);\n    isolation:isolate; /* for overlay stacking */\n  }\n  .mv-card video{ width:100%; height:100%; object-fit:cover; display:block; }\n\n  /* Readability: scrim + caption */\n  .mv-card::after{\n    content:\"\"; position:absolute; inset:0; z-index:1;\n    background:var(--cap-bg);\n    transition:opacity .2s ease;\n  }\n  .mv-caption{\n    position:absolute; inset:auto 10px 10px 10px; z-index:2;\n    color:#fff; display:flex; flex-direction:column; gap:6px;\n  }\n  .mv-badge{\n    align-self:flex-start; font-size:12px; line-height:1;\n    padding:6px 9px; border-radius:999px;\n    background:rgba(255,255,255,.12); border:1px solid rgba(255,255,255,.18);\n    backdrop-filter: blur(6px);\n  }\n  .mv-cap-title{\n    margin:0; font-weight:800; letter-spacing:-.01em;\n    font-size:clamp(14px,3.8vw,18px); line-height:1.15;\n    text-shadow:0 1px 12px rgba(0,0,0,.5);\n  }\n  .mv-cap-meta{\n    margin:0; opacity:.9; font-size:13px;\n  }\n\n  /* Hover niceties (desktop) */\n  @media (hover:hover){\n    .mv-card:hover::after{ opacity:.9; }\n    .mv-card:hover video{ transform:scale(1.02); transition:transform .3s ease; }\n  }\n\n  /* Arrows */\n  .mv-nav{\n    position:absolute; top:50%; transform:translateY(-50%);\n    width:36px; height:36px; border-radius:999px;\n    border:1px solid var(--line); background:#fff; color:#111;\n    display:grid; place-items:center; font-size:20px; font-weight:700;\n    box-shadow:0 6px 18px rgba(0,0,0,.08); z-index:3;\n  }\n  .mv-prev{ left:6px; } .mv-next{ right:6px; }\n  @media (hover:hover){ .mv-nav:hover{ background:#f6f6f6; } }\n\n  /* 2-up feel from 480px */\n  @media (min-width:480px){\n    .mv-card{ flex:0 0 calc(50% - var(--gap)/2); }\n  }\n\n  /* Desktop: grid with taller tiles (4:5) */\n  @media (min-width:900px){\n    .mv-track{\n      overflow:visible; display:grid;\n      grid-template-columns:repeat(5, 1fr); gap:16px; padding:0 0;\n    }\n    .mv-card{ flex:initial; aspect-ratio:4/5; }\n    .mv-nav{ display:none; }\n  }\n</style>\n\n<script>\n  (function(){\n    const track = document.getElementById('mvTrack');\n    if(!track) return;\n\n    // Play/pause only when visible within the rail\n    const vids = track.querySelectorAll('video');\n    const io = new IntersectionObserver((entries)=>{\n      entries.forEach(e=>{\n        const v = e.target;\n        if(e.isIntersecting){ v.play().catch(()=>{}); } else { v.pause(); }\n      });\n    }, { root: track, threshold: 0.6 });\n    vids.forEach(v=>io.observe(v));\n\n    // Arrow scroll by one card\n    const prev = document.querySelector('.mv-prev');\n    const next = document.querySelector('.mv-next');\n    function scrollByCard(dir){\n      const card = track.querySelector('.mv-card');\n      if(!card) return;\n      const cs = getComputedStyle(track);\n      const gap = parseFloat(cs.columnGap || cs.gap || 12);\n      const amount = card.getBoundingClientRect().width + gap;\n      track.scrollBy({ left: dir * amount, behavior: 'smooth' });\n    }\n    prev?.addEventListener('click', ()=>scrollByCard(-1));\n    next?.addEventListener('click', ()=>scrollByCard(1));\n  })();\n</script>",
        "text_alignment": "center"
      }
    },
