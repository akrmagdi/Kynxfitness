    "custom_liquid_GjrQ4d": {
      "type": "custom-liquid",
      "name": "t:sections.custom_liquid.presets.custom_liquid.name",
      "settings": {
        "color_scheme": "",
        "separate_section_with_border": true,
        "remove_vertical_spacing": true,
        "remove_horizontal_spacing": true,
        "liquid": "<!-- KYNX: Minimal Value + Countdown (uses Font Awesome) -->\n<section class=\"kx-mini-value\" id=\"kx-mini-value\">\n  <!-- slim urgency row -->\n  <div class=\"kx-urg\">\n    <span class=\"kx-urg__txt\"><i class=\"fa-solid fa-bolt\"></i> Lifetime access $49.90 — ends in</span>\n    <span class=\"kx-urg__cd\" aria-live=\"polite\">\n      <span class=\"kx-hh\">00</span>:<span class=\"kx-mm\">00</span>:<span class=\"kx-ss\">00</span>\n    </span>\n    <a class=\"kx-urg__btn\" href=\"/cart/41969622843486:1? checkout\">Get it now</a>\n  </div>\n\n  <!-- tiny headline -->\n  <h3 class=\"kx-title\">Pay once. Access for life.</h3>\n\n  <!-- 3 clean FA icon points -->\n  <ul class=\"kx-points\" aria-label=\"Key benefits\">\n    <li>\n      <i class=\"fa-solid fa-mobile-screen-button\" aria-hidden=\"true\"></i>\n      <span>Any device</span>\n    </li>\n    <li>\n      <i class=\"fa-solid fa-dumbbell\" aria-hidden=\"true\"></i>\n      <span>Targeted programs</span>\n    </li>\n    <li>\n      <i class=\"fa-solid fa-infinity\" aria-hidden=\"true\"></i>\n      <span>Lifetime updates</span>\n    </li>\n  </ul>\n</section>\n\n<style>\n  .kx-mini-value{\n    --bg:#0b0b0b; --ink:#e9eef4; --muted:#afbcc6;\n    --line:#1a1b1f; --accent:#0ea5e9; --accent-2:#0891b2;\n    background:var(--bg); color:var(--ink); font-family:inherit;\n    padding:14px 12px;  /* tight */\n  }\n\n  /* urgency row */\n  .kx-urg{\n    display:grid; grid-template-columns:1fr auto auto; gap:8px; align-items:center;\n    background:#101114; border:1px solid var(--line); border-radius:8px;\n    padding:8px 10px; margin:0 auto 10px; max-width:1160px;\n  }\n  .kx-urg__txt{ display:flex; align-items:center; gap:6px; font-weight:700; font-size:12.5px; color:#eaf2f7; white-space:nowrap; overflow:hidden; text-overflow:ellipsis; }\n  .kx-urg__txt .fa-bolt{ font-size:14px; }\n  .kx-urg__cd{ font-weight:800; font-size:13px; letter-spacing:.5px; }\n  .kx-urg__btn{\n    justify-self:end; background:var(--accent); color:#fff; text-decoration:none;\n    font-weight:800; font-size:12.5px; padding:8px 10px; border-radius:999px; white-space:nowrap;\n    border:1px solid rgba(14,165,233,.35);\n  }\n  .kx-urg__btn:hover{ background:var(--accent-2); }\n\n  .kx-title{\n    margin:4px 0 8px; text-align:center; font-weight:800;\n    font-size:clamp(16px,2.8vw,20px); line-height:1.2; color:#e9eef4;\n  }\n\n  /* 3-point list */\n  .kx-points{\n    list-style:none; margin:0 auto; padding:0; max-width:980px;\n    display:grid; gap:8px; grid-template-columns:repeat(3,minmax(0,1fr));\n  }\n  .kx-points li{\n    display:flex; flex-direction:column; align-items:center; justify-content:center;\n    gap:6px; padding:10px 8px;\n    background:#101114; border:1px solid var(--line); border-radius:10px;\n    min-height:64px;\n  }\n  .kx-points i{ font-size:18px; opacity:.95; }\n  .kx-points span{ font-size:12.5px; color:#cdd9e2; font-weight:700; }\n\n  /* mobile */\n  @media (max-width:640px){\n    .kx-urg{ grid-template-columns:1fr auto; }\n    .kx-urg__btn{ grid-column:1 / -1; justify-self:stretch; text-align:center; }\n    .kx-points{ grid-template-columns:repeat(3, minmax(0,1fr)); } /* stays 3-up but tiny */\n    .kx-points li{ min-height:58px; padding:8px 6px; }\n    .kx-points i{ font-size:16px; }\n    .kx-points span{ font-size:12px; }\n  }\n\n  @media (prefers-reduced-motion:reduce){\n    .kx-urg__btn{ transition:none; box-shadow:none; }\n  }\n</style>\n\n<script>\n  (function(){\n    // Scoped countdown: 48h rolling window (use data-deadline if you prefer a fixed end)\n    var root = document.getElementById('kx-mini-value');\n    if(!root) return;\n    var hh = root.querySelector('.kx-hh'),\n        mm = root.querySelector('.kx-mm'),\n        ss = root.querySelector('.kx-ss');\n\n    var KEY = 'kxMiniDeadline';\n    var end;\n    var attr = root.getAttribute('data-deadline'); // e.g. 2025-08-20T23:59:00+02:00\n\n    try{\n      if(attr){\n        end = new Date(attr);\n      }else{\n        var saved = localStorage.getItem(KEY);\n        end = saved ? new Date(saved) : new Date(Date.now() + 48*60*60*1000);\n        if(!saved) localStorage.setItem(KEY, end.toISOString());\n      }\n    }catch(e){\n      end = new Date(Date.now() + 48*60*60*1000);\n    }\n\n    function pad(n){return (n<10?'0':'')+n;}\n    function tick(){\n      var diff = end - Date.now();\n      if(diff <= 0){ hh.textContent='00'; mm.textContent='00'; ss.textContent='00'; return; }\n      var s = Math.floor(diff/1000);\n      var H = Math.floor(s/3600);\n      var M = Math.floor((s%3600)/60);\n      var S = s%60;\n      hh.textContent = pad(H);\n      mm.textContent = pad(M);\n      ss.textContent = pad(S);\n    }\n    tick();\n    setInterval(tick, 1000);\n  })();\n</script>",
        "text_alignment": "center"
      }
    },
    "custom_liquid_w6D9wM": {
      "type": "custom-liquid",
      "name": "t:sections.custom_liquid.presets.custom_liquid.name",
      "settings": {
        "color_scheme": "",
        "separate_section_with_border": false,
        "remove_vertical_spacing": true,
        "remove_horizontal_spacing": true,
        "liquid": "<!-- Mobility Program Builder (Shopify Custom HTML section) -->\n<section class=\"program-builder\">\n  <div class=\"program-builder__wrap\">\n    <!-- Headline -->\n    <header class=\"pb-hero\">\n      <h2 class=\"pb-title\">Build your custom mobility program.</h2>\n      <p class=\"pb-sub\">Create a routine that fits your body, schedule, and goals — accessible on any device.</p>\n    </header>\n\n    <!-- Tab triggers -->\n    <nav class=\"pb-steps\" role=\"tablist\" aria-label=\"Program builder steps\">\n      <button class=\"pb-step is-active\" role=\"tab\" aria-selected=\"true\" aria-controls=\"panel-areas\" id=\"tab-areas\">\n        <span class=\"pb-step__icon\" aria-hidden=\"true\">👤</span> Pick 1–3 areas\n      </button>\n      <button class=\"pb-step\" role=\"tab\" aria-selected=\"false\" aria-controls=\"panel-length\" id=\"tab-length\">\n        <span class=\"pb-step__icon\" aria-hidden=\"true\">⏱</span> Choose your length\n      </button>\n      <button class=\"pb-step\" role=\"tab\" aria-selected=\"false\" aria-controls=\"panel-style\" id=\"tab-style\">\n        <span class=\"pb-step__icon\" aria-hidden=\"true\">☰</span> Pick your session style\n      </button>\n    </nav>\n\n    <!-- Animated content area -->\n    <div class=\"pb-panels\" id=\"pb-panels\">\n      <!-- Areas -->\n      <section class=\"pb-panel is-active\" role=\"tabpanel\" id=\"panel-areas\" aria-labelledby=\"tab-areas\">\n        <ul class=\"pb-grid\">\n          <li><button class=\"pb-chip is-on\">Full Body</button></li>\n          <li><button class=\"pb-chip\">Upper Back</button></li>\n          <li><button class=\"pb-chip\">Lower Back</button></li>\n          <li><button class=\"pb-chip\">Hips</button></li>\n          <li><button class=\"pb-chip\">Shoulders + Neck</button></li>\n          <li><button class=\"pb-chip\">Upper Body</button></li>\n          <li><button class=\"pb-chip\">Wrist + Elbows</button></li>\n          <li><button class=\"pb-chip\">Knees</button></li>\n          <li><button class=\"pb-chip\">Ankles + Calves</button></li>\n        </ul>\n        <p class=\"pb-hint\">Tip: choose up to three focus areas to personalize your plan.</p>\n      </section>\n\n      <!-- Length -->\n      <section class=\"pb-panel\" role=\"tabpanel\" id=\"panel-length\" aria-labelledby=\"tab-length\">\n        <div class=\"pb-card\">\n          <h3 class=\"pb-card__title\">Days of week</h3>\n          <div class=\"pb-pills\">\n            <button class=\"pb-pill is-on\">M</button><button class=\"pb-pill\">T</button>\n            <button class=\"pb-pill is-on\">W</button><button class=\"pb-pill\">T</button>\n            <button class=\"pb-pill is-on\">F</button><button class=\"pb-pill\">S</button>\n            <button class=\"pb-pill\">S</button>\n          </div>\n        </div>\n\n        <div class=\"pb-card\">\n          <h3 class=\"pb-card__title\">Total time per day</h3>\n          <div class=\"pb-options\">\n            <button class=\"pb-option is-on\">6–15 min</button>\n            <button class=\"pb-option\">15–25 min</button>\n            <button class=\"pb-option\">25–40 min</button>\n          </div>\n        </div>\n\n        <div class=\"pb-card\">\n          <h3 class=\"pb-card__title\">Length of program</h3>\n          <div class=\"pb-counter\">\n            <button class=\"pb-ctr\" data-ctr=\"-\">−</button>\n            <div class=\"pb-ctr__value\"><span id=\"weeks\">3</span> weeks</div>\n            <button class=\"pb-ctr\" data-ctr=\"+\">+</button>\n          </div>\n        </div>\n      </section>\n\n      <!-- Style -->\n      <section class=\"pb-panel\" role=\"tabpanel\" id=\"panel-style\" aria-labelledby=\"tab-style\">\n        <article class=\"pb-style\">\n          <h4 class=\"pb-style__title\">More Passive Poses, Some Active</h4>\n          <p class=\"pb-style__body\">Primarily uses static, passive holds with longer durations.</p>\n          <p class=\"pb-style__em\"><strong>Focuses on:</strong> Mindfulness and breath work</p>\n          <p class=\"pb-style__em\"><strong>Ideal for:</strong> Recovery and easing tension</p>\n        </article>\n\n        <article class=\"pb-style pb-style--outline\">\n          <h4 class=\"pb-style__title\">More Active Poses, Some Passive</h4>\n          <p class=\"pb-style__body\">Prioritizes dynamic, active sequences with shorter holds.</p>\n          <p class=\"pb-style__em\"><strong>Focuses on:</strong> Activation and priming the body</p>\n          <p class=\"pb-style__em\"><strong>Ideal for:</strong> Pre-activity or corrective mobility work</p>\n        </article>\n      </section>\n    </div>\n\n    <!-- CTA -->\n    <div class=\"pb-cta\">\n      <div>\n        <h3 class=\"pb-cta__title\">Start your tailored plan with KYNX Mobility</h3>\n        <p class=\"pb-cta__sub\">Stream anywhere. Progress you can feel in minutes a day.</p>\n      </div>\n      <!-- Direct-to-checkout for variant 42022227083341 -->\n      <a class=\"pb-btn\" href=\"/cart/41969622843486:1?return_to=/checkout\">START TODAY! →</a>\n      <!-- To auto-apply a discount, use: /cart/42022227083341:1?discount=MOVE10&return_to=/checkout -->\n    </div>\n  </div>\n</section>\n\n<style>\n  /* Theme tokens (dark, blue accent) */\n  .program-builder{\n    --bg:#0b0b0b; --ink:#f5f5f5; --muted:#a3a3a3;\n    --panel:#111214; --panel-2:#0f1012;\n    --line:rgba(255,255,255,.12);\n    --radius:14px;\n    --accent:#0ea5e9; --accent-2:#0891b2;\n    background:var(--bg); color:var(--ink); padding:28px 16px;\n  }\n  .program-builder__wrap{ max-width:1100px; margin:0 auto; }\n\n  .pb-hero{ border:1px solid var(--line); border-radius:var(--radius); padding:22px 20px; background:var(--panel-2); margin-bottom:20px; }\n  .pb-title{ margin:0 0 8px; font-size:clamp(26px,5.5vw,44px); line-height:1.1; font-weight:800; }\n  .pb-sub{ margin:0; color:var(--muted); font-size:clamp(14px,1.6vw,18px); }\n\n  /* Steps (tabs) */\n  .pb-steps{ display:grid; grid-template-columns:1fr; gap:10px; padding:8px 0 14px; border-bottom:1px solid var(--line); }\n  .pb-step{\n    appearance:none; background:none; color:var(--ink);\n    border:1px solid var(--line); border-radius:12px;\n    padding:12px 14px; text-align:left; font-weight:600; cursor:pointer;\n    display:flex; align-items:center; gap:10px; transition:border-color .15s ease, color .15s ease, background-color .15s ease;\n  }\n  .pb-step__icon{ width:18px; height:18px; display:inline-block; filter:grayscale(0.1); }\n  .pb-step:hover{ border-color:rgba(14,165,233,.45); }\n  .pb-step.is-active{ background:#0c1014; border-color:rgba(14,165,233,.6); box-shadow:0 0 0 1px rgba(14,165,233,.25) inset; }\n\n  /* Panels wrapper with height animation */\n  .pb-panels{ position:relative; overflow:hidden; transition:height .28s ease; }\n  .pb-panel{ opacity:0; transform:translateY(8px); pointer-events:none; position:absolute; left:0; right:0; top:0; padding:16px 0; }\n  .pb-panel.is-active{ opacity:1; transform:translateY(0); pointer-events:auto; position:relative; }\n\n  /* Areas grid */\n  .pb-grid{ list-style:none; margin:0; padding:0; display:grid; gap:10px; grid-template-columns:repeat(2,minmax(0,1fr)); }\n  .pb-chip{\n    width:100%; padding:12px 12px; border-radius:12px; color:var(--ink);\n    background:var(--panel); border:1px solid var(--line); cursor:pointer; font-weight:600;\n    transition:border-color .15s ease, background-color .15s ease;\n  }\n  .pb-chip:hover{ border-color:rgba(255,255,255,.25); }\n  .pb-chip.is-on{ border-color:rgba(14,165,233,.6); box-shadow:0 0 0 1px rgba(14,165,233,.25) inset; }\n  .pb-hint{ margin:10px 2px 0; color:var(--muted); font-size:13px; }\n\n  /* Length selectors */\n  .pb-card{ background:var(--panel); border:1px solid var(--line); border-radius:12px; padding:14px; margin-bottom:12px; }\n  .pb-card__title{ margin:0 0 10px; font-size:14px; color:#d8d8d8; font-weight:700; letter-spacing:.2px; }\n  .pb-pills{ display:flex; gap:8px; flex-wrap:wrap; }\n  .pb-pill{ background:#0f1114; border:1px solid var(--line); color:var(--ink); width:40px; height:40px; border-radius:999px; font-weight:700; }\n  .pb-pill.is-on{ border-color:rgba(14,165,233,.6); }\n  .pb-options{ display:flex; flex-wrap:wrap; gap:10px; }\n  .pb-option{ padding:10px 14px; border-radius:999px; border:1px solid var(--line); background:#0f1114; color:var(--ink); font-weight:700; }\n  .pb-option.is-on{ border-color:rgba(14,165,233,.6); }\n  .pb-counter{ display:flex; align-items:center; gap:12px; }\n  .pb-ctr{ width:40px; height:40px; border-radius:999px; border:1px solid var(--line); background:#0f1114; font-size:20px; }\n  .pb-ctr__value{ min-width:120px; text-align:center; font-weight:700; }\n\n  /* Styles list */\n  .pb-style{ background:#111215; border:1px solid var(--line); border-radius:12px; padding:14px; margin-bottom:12px; }\n  .pb-style--outline{ background:transparent; }\n  .pb-style__title{ margin:0 0 8px; font-size:16px; font-weight:800; }\n  .pb-style__body{ margin:0 0 10px; color:#cfcfcf; }\n  .pb-style__em{ margin:0 0 6px; color:#b9b9b9; }\n\n  /* CTA */\n  .pb-cta{ margin-top:16px; background:var(--panel-2); border:1px solid var(--line); border-radius:12px; padding:16px; display:grid; grid-template-columns:1fr; gap:14px; align-items:center; }\n  .pb-cta__title{ margin:0 0 4px; font-size:clamp(16px,2.2vw,22px); }\n  .pb-cta__sub{ margin:0; color:var(--muted); }\n  .pb-btn{ justify-self:start; display:inline-flex; align-items:center; gap:8px; padding:12px 18px; background:var(--accent); color:#fff; border-radius:999px; border:1px solid rgba(14,165,233,.4); box-shadow:0 8px 24px rgba(14,165,233,.25); text-decoration:none; font-weight:800; }\n  .pb-btn:hover{ background:var(--accent-2); }\n\n  /* Layout at >= 900px */\n  @media (min-width:900px){\n    .program-builder{ padding:40px 20px; }\n    .pb-steps{ grid-template-columns:repeat(3,1fr); gap:12px; }\n    .pb-grid{ grid-template-columns:repeat(3,minmax(0,1fr)); }\n    .pb-cta{ grid-template-columns:1fr auto; }\n  }\n</style>\n\n<script>\n  (function(){\n    const tabs = document.querySelectorAll('.pb-step');\n    const panels = document.querySelectorAll('.pb-panel');\n    const wrapper = document.getElementById('pb-panels');\n    const weeksEl = document.getElementById('weeks');\n\n    function setWrapperHeight(panel){\n      const h = panel.scrollHeight;\n      wrapper.style.height = h + 'px';\n    }\n    // init\n    const activePanel = document.querySelector('.pb-panel.is-active');\n    setWrapperHeight(activePanel);\n\n    tabs.forEach(btn=>{\n      btn.addEventListener('click', ()=>{\n        if(btn.classList.contains('is-active')) return;\n        tabs.forEach(b=>{ b.classList.remove('is-active'); b.setAttribute('aria-selected','false'); });\n        btn.classList.add('is-active'); btn.setAttribute('aria-selected','true');\n\n        const target = document.getElementById(btn.getAttribute('aria-controls'));\n        panels.forEach(p=>p.classList.remove('is-active'));\n        target.classList.add('is-active');\n        setWrapperHeight(target);\n      });\n    });\n\n    // simple chip/pill toggles\n    document.addEventListener('click', (e)=>{\n      if(e.target.classList.contains('pb-chip') || e.target.classList.contains('pb-pill') || e.target.classList.contains('pb-option')){\n        e.target.classList.toggle('is-on');\n      }\n      if(e.target.classList.contains('pb-ctr')){\n        const dir = e.target.dataset.ctr;\n        let v = parseInt(weeksEl.textContent,10);\n        v = dir==='+' ? Math.min(12, v+1) : Math.max(1, v-1);\n        weeksEl.textContent = v;\n      }\n    });\n\n    // Recompute height on resize (for responsive rows)\n    window.addEventListener('resize', ()=>{\n      const p = document.querySelector('.pb-panel.is-active');\n      setWrapperHeight(p);\n    });\n  })();\n</script>",
        "text_alignment": "start"
      }
    },
