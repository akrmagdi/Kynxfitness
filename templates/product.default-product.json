ment.addEventListener('change', (e)=>{\n    if(e.target.matches('form[action^=\"\/cart\/add\"] [name^=\"options\"], form[action^=\"\/cart\/add\"] select, form[action^=\"\/cart\/add\"] input[name=\"id\"]')){\n      setTimeout(updateSelectedId, 0);\n    }\n  });\n\n  \/\/ Many themes dispatch a custom event on variant change; catch it if available\n  document.addEventListener('variant:changed', updateSelectedId);\n  document.addEventListener('product:variant-change', updateSelectedId);\n\n  \/\/ Re-run on page transitions (AJAX sections)\n  document.addEventListener('shopify:section:load', updateSelectedId);\n})();\n<\/script>"}}},"block_order":["vendor","title","price","payment_terms","separator","description","variant_picker","buy_buttons","complementary_products","liquid_QfH3Fn"],"settings":{"color_scheme":"","container_size":"lg","product_info_size":35,"show_sticky_add_to_cart":true,"center_basic_info":false,"desktop_media_layout":"grid","desktop_media_grid_gap":30,"mobile_controls":"dots","enable_media_autoplay":false,"enable_video_looping":true,"enable_image_zoom":true,"max_image_zoom_level":3}},"related-products":{"type":"related-products","disabled":true,"settings":{"color_scheme":"","separate_section_with_border":true,"hide_product_information":false,"stack_products":false,"recommendations_count":10,"products_per_row_mobile":"2","products_per_row_desktop":4,"horizontal_spacing_factor":1,"vertical_spacing_factor":1,"subheading":"","title":"Related products","content":""}},"rich_text_VUH4Wt":{"type":"rich-text","blocks":{"heading_44z8Dc":{"type":"heading","settings":{"text":"<strong>NEW PROGRAM EACH MONTH <\/strong>","heading_tag":"h1"}}},"block_order":["heading_44z8Dc"],"name":"t:sections.rich_text.presets.rich_text.name","settings":{"color_scheme":"","separate_section_with_border":true,"content_width":"sm","text_position":"center","remove_vertical_spacing":false}},"slideshow_dyjzVF":{"type":"slideshow","blocks":{"image_RHTNrz":{"type":"image","settings":{"sub